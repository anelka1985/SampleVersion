        </ul>
        
    </div>
    
   <!--心情-->
	<div class="mood color_4 mood_mar">
    	<div class="btu_zl">
        	<div class="btu_gdl pos">
            	<div id="secMenu0" class="btu_game_select pos"><a id="rankTodayId"><span class="btu_man_com2 btu_green_s_com btu_green_ss_left"></span><span class="btu_green_ss_right"></span>今日</a></div>
            </div>
            <div class="btu_gdl pos">
            	<div id="secMenu1" class="btu_game  pos"><a id="rankWeekId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>本周</a></div>
            </div>
            <div class="btu_gdl pos">
            	<div id="secMenu2" class="btu_game pos"><a id="rankMonthId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>本月</a></div>
            </div>
            <div class="btu_gdl pos">
            	<div id="secMenu3" class="btu_game pos"><a id="rankGlobalId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>总榜</a></div>
			</div>
        </div>
        
        <span class="mood_absolute mood_top_left"></span><span class="mood_absolute mood_top_right"></span><span class="mood_absolute mood_bottom_left"></span><span class="mood_absolute mood_bottom_right"></span></a>

    </div>
    
    <!--主题-->
    <div class="headerheader_game">
        <h2 class="color_2 font_20 float_l">
        	<P  id="myRankTextId"></P>
            <P class="P_tb_10"><span id="myRankId" ></span><span id="otherRankId" class="color_4"><!-- other name --></span></P>
        </h2>
    </div>

    
    
    <!--详情-->
    <div id="listDetail" class="weibo beento" style="clear:both;">
        <!-- detail data list -->
    </div>
    
	<!--分页-->
      	<div class="m_auto p_b_20 blohi">  
            <div id="tailButId" class="btu_page btu_green_s pos">
                <a id="tailPageId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>末 页</a>
            </div>
            
            <div id="nextButId" class="btu_page btu_green_s pos">
