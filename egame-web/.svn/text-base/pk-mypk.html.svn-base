<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//WAPFORUM//DTD XHTML Mobile 1.1//EN" "http://www.openmobilealliance.org/tech/DTD/xhtml-mobile11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="pragma" content="no-cache"> 
<meta http-equiv="cache-control" content="no-cache"> 
<meta http-equiv="expires" content="0">
<meta content="telephone=no" name="format-detection" /> 
<title>爱游戏--PK</title>
<link href="css/common.css" rel="stylesheet" type="text/css" />
<script language="javascript">

<!--

if(screen.width<300) {

    document.write('<link href="css/w_240.css" rel="stylesheet" type="text/css" />');

}

else {

    document.write('<link href="css/w_480.css" rel="stylesheet" type="text/css" />');

}

//-->

</script>
<!--通用的JS-->
<script src="js/jquery-1.7.1.js" type="text/javascript"></script>
<script src="js/callAndroid.js" type="text/javascript"></script>
<script src="js/common.js" type="text/javascript"></script>
<script src="js/common_html.js" type="text/javascript"></script>
<script src="js/wqz-globalObject.js" language="javascript" type="text/javascript"></script>
<script src="js/pk-mypk.js" language="javascript" type="text/javascript"></script>
</head>


<body>
	
	<!--title-->
    <div class="title_pk">
        
        <!--所有PK-->
        <ul>
        	<li class="title_pk_nor"><a id="pkIndexId">所有PK</a></li>
            <li class="title_pk_select"><a id="pkMypkId"><span class="head_mask_pknarrow"></span>我的PK</a></li>
        </ul>
        
    </div>

	<!--您的PK-->
	<div class="pri_setting" onclick="">
    	<span class="probdnu_absolute probdnu_top_left"></span><span class="probdnu_absolute probdnu_top_right"></span><span class="probdnu_absolute probdnu_bottom_left"></span><span class="probdnu_absolute probdnu_bottom_right"></span>      
    	
        <div class="p_10 blohi pos">您的PK币剩余<span id="myPkbiId" class="color_3"><!-- my balance pkbi--></span></div>
        
        <!--PK-->
    	<div class="p_10 blohi">
    
            <!--兑换PK币-->
            <div class="btu_top float_l">
                <div class="btu_fri_bg btu_g50 pos m_r_10"><a id="pkChangeId">
                    <span class="btu_grd_l"></span>
                    <span class="btu_grd_r"></span>
                    <span class="icon_dh">兑换PK币</span>
                </a></div>
            </div>
            
            <!--发起-->
            <div class="btu_top float_l">
                <div class="btu_fri_bg btu_g50 pos m_r_10"><a id="pkModeId">
                    <span class="btu_grd_l"></span>
                    <span class="btu_grd_r"></span>
                    <span class="icon_pk">发起PK</span>
                </a></div>
            </div>
        
		</div>

        
    </div>   
     
    <!--详情-->
    <div class="beento m_1_10">
        
        <table class="dunit1 m_tb_10">
        	<tr class="dunit1_out1">
            	<td class="dunit1_inner4 table_line">发起时间</td>
                <td class="table_line">奖金（PK币）</td>
                <td class="">操作</td>
            </tr>
            <tbody id="listDetail">
	           <!-- detail list data -->
			</tbody>
        </table>
        
        <!--查看PK记录-->
        <div class="btu_g pos"><a id="pkRecord">
            <span class="btu_g_l"></span>
            <span class="btu_g_r"></span>
            查看PK记录
        </a></div>
                
         <!--分页-->
      	<div class="m_auto p_b_20 blohi">  
            <div id="tailButId" class="btu_page btu_green_s pos">
                <a id="tailPageId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>末 页</a>
            </div>
            
            <div id="nextButId" class="btu_page btu_green_s pos">
                <a id="nextPageId"><span class="btu_man_com2 btu_green_s_com btu_green_s_left"></span><span class="btu_green_s_right"></span>下一页</a>
            </div>
            
            <div id="previousButId" class="btu_page btu_gra_s pos btu_page_com">
                <a id="previousPageId"><span class="btu_man_com2 btu_gra_s_com btu_gra_s_left"></span><span class="btu_gra_s_right"></span>上一页</a>
            </div>
            
            <div id="firstButId" class="btu_page btu_gra_s pos btu_page_com">
                <a id="firstPageId"><span class="btu_man_com2 btu_gra_s_com btu_gra_s_left"></span><span class="btu_gra_s_right"></span>首 页</a>
            </div>
        </div>
    </div>
</body>
</html>

