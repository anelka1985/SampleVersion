.btu_bq { width:30%; }

.fri_bdnu { display: block; overflow: hidden; margin:0 10px; float:right; }
.fri_bdnu a { display:block;}
.fri_bdnu_bg { background-color:#dbdbdb; }
.fri_sel { width:100px; height:30px;}

/* 广场 */
.square_tit li{ float:left; display:block; overflow:hidden;}
.square_tit_w1 { width:33%; }
.square_tit_w2 { width:34%; }

.square_a a { width:auto; height:auto; display:block;}

.sq_v { width:auto; margin:4px 0 5px 5px; font-weight:bold;}
.square_vb { height:10px; border-bottom:#fff solid 1px; margin:5px;}
.color_9,.color_9 a { color:#397222;}

.square_po a { width:auto; height:auto; display:block;}

.loading,.topic_auto { margin:0 auto;}

.act_narrow_r { position:absolute; right:10px; top:25px; }
.bg_act { background:#dfdfdf url(../images/bg_act.png) repeat-x top left;}

/* 主题详细 */
.icon_triangle { background: url("../images/icon_triangle.png") no-repeat ; }

/* 游戏主页 */
.btu_game_mar { margin:10px 10px 15px;}

.eva_star,.keynote { margin:5px 10px; }
.eva_star { padding:8px 0 0 0;}
.height_game { height:200px; }

.star_b_bg{ width:232px; height:40px; background:url(../images/star_g_b.png) no-repeat; margin:0 auto;}
.star_y_b { width:232px; height:40px; overflow:hidden; background:url(../images/star_y_b.png) no-repeat; }
.star_y { width:98px; height:17px; overflow:hidden; background:url(../images/star_y.png) no-repeat;}

/* 达人榜 */
.game_integral { position:absolute; top:50%; right:40px; margin:-7px 0 0 0 ; }
.line_2 {  border-bottom: 1px solid #FFFFFF;  border-top: 1px solid #afafaf;}

/* 隐私设置 */
.pri_setting { padding:0 10px; line-height:30px; }
.pri_setmar { padding:0 10px; margin:0 10px 10px 10px; }

.ps_input,.pk_input { width:300px; height:37px; background:url(../images/ps_input_mid.png) repeat-x; }
.ps_input_left { width:10px; height:37px; background:url(../images/ps_input_left.png) no-repeat; }
.ps_input_right { width:10px; height:37px; background:url(../images/ps_input_right.png) no-repeat;}
.ps_inp,.pk_inp { width:280px; height:37px; line-height:37px; border:none; background:none; padding:0 10px;}

/* PK */
.dunit1_inner4 { width:50%;}
.pk_input { width:350px; }
.pk_inp { width:330px; }
.pk_input2 { width:440px; margin:0 auto; }
.pk_inp2 { width:420px; }

.icon_bq_b { width:100px; height:100px; margin:0 auto; }
.vs { height:140px; background:url(../images/vs.png) no-repeat; }

.css_game_link,.css_active_link,.css_topic_link,.css_news_link,.css_friend_link,.css_wap_link {  color: #AC6A00; }

/* 多选框 单选框 */
.icon_checkbox1,.icon_checkbox2 { width:24px; height:24px; }
.icon_checkbox1 { background:url(../images/icon_checbox1.png) no-repeat; }
.icon_checkbox2 { background:url(../images/icon_checbox2.png) no-repeat; }

.icon_radio1,.icon_radio2 { width:24px; height:24px; }
.icon_radio1 { background:url(../images/icon_radio1.png) no-repeat; }
.icon_radio2 { background:url(../images/icon_radio2.png) no-repeat; }










